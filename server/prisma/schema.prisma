// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider     = "mysql"
  url          = env("DATABASE_URL")
  relationMode = "prisma"
}

model Usuario {
  ID          Int
  username    String
  password    String
  name        String
  surname     String
  mail        String
  regdate     DateTime
}

model Estacionamientos {
  ID          Int
  adress      String
  type        String
  capacity    Int
}

model Reservas {
  ID          Int
  userId      Int
  parkingID   Int
  resdate     DateTime
  resduration Int
}

